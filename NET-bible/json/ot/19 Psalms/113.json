[
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\"><st data-num=\"01984\" class=\"\">Praise</st> the <span class=\"smcaps\"><st data-num=\"03050\" class=\"\">Lord</st></span>.<p class=\"poetry\"><st data-num=\"01984\" class=\"\">Praise</st>, you <st data-num=\"05650\" class=\"\">servants</st> of the <span class=\"smcaps\"><st data-num=\"03068\" class=\"\">Lord</st></span>,<p class=\"poetry\"><st data-num=\"01984\" class=\"\">praise</st> the <st data-num=\"08034\" class=\"\">name</st> of the <span class=\"smcaps\"><st data-num=\"03068\" class=\"\">Lord</st></span>. </p>",
    "verse": "1"
  },
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\">May the <span class=\"smcaps\"><st data-num=\"03068\" class=\"\">Lord</st></span>\u2019s <st data-num=\"08034\" class=\"\">name</st> <st data-num=\"01961\" class=\"\">be</st> <st data-num=\"01288\" class=\"\">praised</st><p class=\"poetry\"><st data-num=\"06258\" class=\"\">now</st> and <st data-num=\"05704 05769\">forevermore</st>. </p>",
    "verse": "2"
  },
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\">From <st data-num=\"04217 08121\">east</st> <st data-num=\"05704\" class=\"\">to</st> <st data-num=\"03996\" class=\"\">west</st><n id=\"1\" /> <p class=\"poetry\">the <span class=\"smcaps\"><st data-num=\"03068\" class=\"\">Lord</st></span>\u2019s <st data-num=\"08034\" class=\"\">name</st> is <st data-num=\"01984\" class=\"\">deserving of praise</st>. </p>",
    "verse": "3"
  },
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\">The <span class=\"smcaps\"><st data-num=\"03068\" class=\"\">Lord</st></span> is <st data-num=\"07311\" class=\"\">exalted</st> <st data-num=\"05921\" class=\"\">over</st> <st data-num=\"03605\" class=\"\">all</st> the <st data-num=\"01471\" class=\"\">nations</st>;<p class=\"poetry\">his <st data-num=\"03519\" class=\"\">splendor</st> reaches <st data-num=\"05921\" class=\"\">beyond</st> the <st data-num=\"08064\" class=\"\">sky</st>.<n id=\"1\" /> </p>",
    "verse": "4"
  },
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\"><st data-num=\"04310\" class=\"\">Who</st> can compare to the <span class=\"smcaps\"><st data-num=\"03068\" class=\"\">Lord</st></span> our <st data-num=\"0430\" class=\"\">God</st>,<p class=\"poetry\">who <st data-num=\"03427\" class=\"\">sits</st> on a <st data-num=\"01361\" class=\"\">high</st> throne?<n id=\"1\" /> </p>",
    "verse": "5"
  },
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\">He <st data-num=\"08213\" class=\"\">bends down</st> to <st data-num=\"07200\" class=\"\">loo</st>k<n id=\"1\" /> <p class=\"poetry\">at the <st data-num=\"08064\" class=\"\">sky</st> and the <st data-num=\"0776\" class=\"\">earth</st>. </p>",
    "verse": "6"
  },
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\">He <st data-num=\"06965\" class=\"\">raises</st> the <st data-num=\"01800\" class=\"\">poor</st> from the <st data-num=\"06083\" class=\"\">dirt</st><p class=\"poetry\">and <st data-num=\"07311\" class=\"\">lifts up</st> the <st data-num=\"034\" class=\"\">needy</st> from the <st data-num=\"0830\" class=\"\">garbage pile</st><n id=\"1\" /></p>",
    "verse": "7"
  },
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\">that he might <st data-num=\"03427\" class=\"\">seat</st> him <st data-num=\"05973\" class=\"\">with</st> <st data-num=\"05081\" class=\"\">princes</st>,<p class=\"poetry\"><st data-num=\"05973\" class=\"\">with</st> the <st data-num=\"05081\" class=\"\">princes</st> of his <st data-num=\"05971\" class=\"\">people</st>. </p>",
    "verse": "8"
  },
  {
    "bookname": "Psalms",
    "chapter": "113",
    "text": "<p class=\"poetry\">He <st data-num=\"03427\" class=\"\">makes</st> the <st data-num=\"06135\" class=\"\">barren woman</st> of the <st data-num=\"01004\" class=\"\">family</st><n id=\"1\" /> <p class=\"poetry\">a <st data-num=\"08056\" class=\"\">happy</st> <st data-num=\"0517\" class=\"\">mother</st> of <st data-num=\"01121\" class=\"\">children</st>.<n id=\"2\" /> <p class=\"poetry\"><st data-num=\"01984\" class=\"\">Praise</st> the <st data-num=\"03050\" class=\"\"><span class=\"smcaps\">Lord</span></st>. </p>",
    "verse": "9"
  }
]